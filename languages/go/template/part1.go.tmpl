package main

import (
	"bufio"
	"fmt"
	"os"
	"time"
)

func main() {
	start := time.Now()
	result, err := solve("inputs/input.txt")
	elapsed := time.Since(start)
	if err != nil {
		panic(err)
	}
	fmt.Println(result)
	fmt.Fprintf(os.Stderr, "Runtime: %dms\n", elapsed.Milliseconds())
}

func solve(inputPath string) (int, error) {
	file, err := os.Open(inputPath)
	if err != nil {
		return 0, err
	}
	defer file.Close()

	scanner := bufio.NewScanner(file)
	
	// TODO: Implement solution
	for scanner.Scan() {
		line := scanner.Text()
		_ = line
	}

	if err := scanner.Err(); err != nil {
		return 0, err
	}

	return 0, nil
}
